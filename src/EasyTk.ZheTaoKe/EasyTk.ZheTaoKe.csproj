  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\EasyTk.Core\EasyTk.Core.csproj" />
    <ProjectReference Include="..\EasyTk\EasyTk.csproj" />
  </ItemGroup>

</Project>
